{
  "name": "openai-text-prompts",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-scripts": "5.0.1",
    "node-fetch": "^2.0.0"
  },
  "scripts": {
    "prestart": "yarn --no-bin-links install",
    "start": "PORT=\"$FRONTEND_PORT\" react-scripts start",
    "start:lambda": "netlify-lambda serve src/lambda",
    "build": "yarn build:app & yarn build:lambda",
    "build:app": "react-scripts build",
    "build:lambda": "netlify-lambda build src/lambda"
  },
  "devDependencies": {
    "babel-loader": "^8.0.4",
    "http-proxy-middleware": "^0.19.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
